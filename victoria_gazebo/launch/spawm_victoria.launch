<?xml version="1.0"?>
<!-- Add License -->
<launch>

  <arg name="x" default="0.0" />
  <arg name="y" default="0.0" />
  <arg name="z" default="0.0" />
  <arg name="yaw" default="0.0" />


  <!-- URDF and TF support -->
  <param name="robot_description" command="$(find xacro)/xacro.py $(find victoria_description)/robots/victoria.xacro" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" >
    <param name="publish_frequency" value="100.0"/>
  </node>

  <!-- Add default controllers if needed -->

  <!-- Spawn robot in gazebo -->
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
        args="-urdf -x $(arg x) -y $(arg y) -z $(arg z) -Y $(arg yaw) -model victoria -param robot_description"/>

  <!-- if we need to launch an startup script --> 
  <!-- node name="start_robot" pkg="victoria_gazebo" type="start_simulated_robot.py" /-->

</launch>

